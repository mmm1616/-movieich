<%- if controller_name != 'sessions' %>
  <%= link_to "ログインはこちら", new_session_path(resource_name), class: "btn btn-outline-dark" %><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "新規登録はこちら", new_registration_path(resource_name), class: "btn btn-outline-dark mt-2" %><br />
<% end %>

